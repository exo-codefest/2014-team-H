    <form class="form-horizontal" id="form-horizontal">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">&{task-name.label}</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="inputEmail3" placeholder="Project-name">
        </div>
    </div>
    <div class="form-group" >
        <label for="inputEmail3" class="col-sm-2 control-label">&{description.label}</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="inputEmail4" placeholder="Project-Team-Lead">
        </div>
    </div>


    <div class="modal-footer">
        <button  type="submit" style="text-align:center;" class="CancelButton btn-default">&{cancel.label}</button>
        <button type="button" style="text-align:center;" class="addtassk btn-default" >&{save.label}</button>
    </div>
        </form>
